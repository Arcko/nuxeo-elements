{
  "description": "Nuxeo Web Components",
  "keywords": [
    "nuxeo",
    "web-components",
    "polymer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nuxeo/nuxeo-elements.git"
  },
  "name": "@nuxeo/nuxeo-elements",
  "version": "3.0.0-SNAPSHOT",
  "contributors": [
    "Nelson Silva <nsilva@nuxeo.com>"
  ],
  "license": "Apache 2.0",
  "scripts": {
    "eslint": "eslint *.html demo/**/*.html test/**/*.html",
    "polylint": "polymer lint",
    "lint": "npm run eslint && npm run polylint",
    "test": "polymer test --npm --simpleOutput --expanded -l chrome",
    "docs": "polymer analyze > analysis.json && polymer serve",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "polymer-cli": "^1.8.0",
    "wct-istanbul": "^0.14.3",
    "@polymer/iron-component-page": "^4.0.0",
    "@polymer/test-fixture": "^4.0.0",
    "wct-browser-legacy": "^1.0.1",
    "@webcomponents/webcomponentsjs": "^2.0.0"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "main": "nuxeo-elements.js",
  "dependencies": {
    "@polymer/polymer": "^3.0.0",
    "nuxeo": "^3.2.1",
    "es6-promise": "^4.0.0"
  }
}
